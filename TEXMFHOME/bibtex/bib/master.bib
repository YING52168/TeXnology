@article{vanrooij2004,
	Abstract = {In terms of Groenendijk and Stokhof's (1984) formalization of exhaustive interpretation, many conversational implicatures can be accounted for. In this paper we justify and generalize this approach. Our justification proceeds by relating their account via Halpern and Moses's (1984) nonmonotonic theory of 'only knowing' to the Gricean maxims of quality and the first submaxim of quantity. The approach of Groenendijk and Stokhof (1984) is generalized such that it can also account for implicatures that are triggered in subclauses not entailed by the whole complex sentence.},
	Author = {van Rooij, Robert and Schulz, Katrin},
	Date-Added = {2013-07-05 13:32:14 +0000},
