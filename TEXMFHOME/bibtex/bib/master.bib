@article{papafragou2003,
	Abstract = {Presents 2 sets of experiments designed to investigate the acquisition of scalar implicatures. Recent studies suggest that preschool children are often insensitive to such implicatures when they interpret scalar terms. This conclusion raises 2 important questions: (1) are all scalar terms treated in the same way by young children?, and (2) does the child's difficulty reflect a genuine inability to derive scalar implicatures? Exp 1 addresses the 1st question by testing a group of 30 5-yr-olds and 30 adults (all native speakers of Greek) on 3 scales. In each case, subjects were presented with contexts which satisfied the semantic content of the stronger terms on each scale but were described using the weaker terms of the scales. We found that, while adults mostly rejected these infelicitous descriptions, children almost never did so. Children also differed from adults in that their rejection rate on the numerical scale was reliably higher than on the 2 other scales. In order to address question (2), we trained a group of 30 5-yr-olds to detect infelicitous statements. We then presented them with modified versions of the stories which now more readily invited scalar inferences. These manipulations gave rise to significantly higher rejection rates than those observed in Exp 1.},
	Author = {Papafragou, Anna and Musolino, Julien},
	Date-Added = {2013-09-10 05:06:38 +0000},
	Date-Modified = {2013-09-10 05:10:23 +0000},
	Doi = {10.1016/S0010-0277(02)00179-8},
	Journal = {Cognition},
	Keywords = {semantics, pragmatics, semantics-pragmatics interface, scalar implicatures},
	Month = {January},
	Number = {3},
	Pages = {253-282},
	Title = {Scalar implicatures: Experiments at the semantics-pragmatics interface},
	Volume = {86},
	Year = {2003},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/S0010-0277(02)00179-8}}

@article{noveck2001,
	Abstract = {A conversational implicature is an inference that consists of attributing to a speaker an implicit meaning that goes beyond the explicit linguistic meaning of an utterance. This paper experimentally investigates scalar implicature, a paradigmatic case of implicature in which a speaker's use of a term like some indicates that the speaker had reasons not to use a more informative term from the same scale, eg, all; thus, some implicates not all. Pragmatic theorists like Grice would predict that a pragmatic interpretation is determined only after its explicit, logical meaning is incorporated (eg, where some means ``at least one''). The present work aims to developmentally examine this prediction by showing how younger, albeit competent, reasoners initially treat a relatively weak term logically before becoming aware of its pragmatic potential. Experiment 1 presents a modal reasoning scenario offering an exhaustive set of conclusions; critical among these is participants' evaluation of a statement expressing ``might be x'' when the context indicates that the stronger ``must be x'' is true. The conversationally infelicitous ``might be x'' can be understood logically (eg, as compatible with must) or pragmatically (as exclusive to must). Results from children ages 5, 7, \& 9, as well as adults, revealed that (1) 7-year-olds are the youngest to demonstrate modal competence overall, \& (2) 7- \& 9-year-olds treat the infelicitous might logically significantly more often than adults do. Experiment 2 showed how training with the modal task can suspend the implicatures for adults. Experiment 3 provided converging evidence of the developmental pragmatic effect with the French existential quantifier certains (some). While linguistically sophisticated children (ages 8 \& 10) typically treat certains as compatible with tous (all), adults are equivocal. These results, which are consistent with unanticipated findings in classic developmental papers, reveal a consistent ordering in which representations of weak scalar terms tend to be treated logically by young competent participants \& more pragmatically by older ones. This work is also relevant to the treatment of scalar implicatures in the reasoning literature.},
	Author = {Noveck, Ira A},
	Date-Added = {2013-09-10 05:00:43 +0000},
	Date-Modified = {2013-09-10 05:03:33 +0000},
	Doi = {10.1016/S0010-0277(00)00114-1},
	Journal = {Cognition},
	Keywords = {pragmatics, semantics, scalar implicatures, acquisition},
	Month = {February},
	Number = {2},
	Pages = {165-188},
	Title = {When Children Are More Logical Than Adults: Experimental Investigations of Scalar Implicature},
	Volume = {78},
	Year = {2001},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/S0010-0277(00)00114-1}}

