@article{nakanishi2004,
	Abstract = {This paper provides a semantic analysis of morphological plurals with the suffix -tati in Japanese. Although many of the previous analyses treat a -tati plural on a par with a -men plural in Mandarin Chinese, which is inherently definite, we present ample evidence that a -tati plural is unmarked for definiteness. Instead, we analyze a -tati plural as a non-uniform plural whose extension can include entities that are not in the extension of the common noun to which -tati is attached. This proposal proves extremely successful in accounting for a variety of otherwise puzzling properties of a -tati plural, including the lack of generic/kind readings & the tendency to take wide scope. 25 . Adapted from the source document},
	Author = {Nakanishi, Kimiko and Tomioka, Satoshi},
	Date-Added = {2013-03-10 22:58:57 +0000},
	Date-Modified = {2013-12-07 15:32:39 +0000},
	Journal = {Journal of East Asian Linguistics},
	Keywords = {Japanese, plurals},
	Number = {2},
	Pages = {113-140},
	Title = {{J}apanese Plurals Are Exceptional},
	Volume = {13},
	Year = {2004}}

@article{kang1994,
	Abstract = {An attempt is made to present the semantics of Korean common nouns in light of Godehard Link's (1983) semantic theory on denotations of plurals & mass terms. It is shown that the semantic count/mass distinction of common nouns is as important in Korean as in English, despite the fact that the distinction is blurred in some constructions. The semantic domains of Korean count nouns are shown to be similar to those of English count nouns except that the denotation of a syntactically singular count noun in Korean may include a semantically plural as well as a singular domain. It is also shown that the semantic domains of common nouns, in Korean & even in English in some cases, are much more flexible than may be thought. This claim is made with respect to the proportional reading of English many & Korean manhun. In this connection, a possible semantic treatment of Korean classifiers as domain shifters is provided. Some problems closely related to the interpretation of common nouns, ie, the problems of kinds, genericity, & topicality, are discussed. It is shown how these notions are related in the theory that allows flexible interpretive domains of common nouns. 28 . Adapted from the source document},
	Author = {Beom-Mo Kang},
	Date-Added = {2013-03-10 22:56:43 +0000},
	Date-Modified = {2013-10-17 22:46:54 +0000},
	Journal = {Journal of East Asian Linguistics},
	Keywords = {Korean, plurals},
	Number = {1},
	Pages = {1-24},
	Title = {Plurality and Other Semantic Aspects of Common Nouns in {K}orean},
	Volume = {3},
	Year = {1994}}

@article{hudsonkam2009,
	Abstract = {When natural language input contains grammatical forms that are used probabilistically and inconsistently, learners will sometimes reproduce the inconsistencies; but sometimes they will instead regularize the use of these forms, introducing consistency in the language that was not present in the input. In this paper we ask what produces such regularization. We conducted three artificial language experiments, varying the use of determiners in the types of inconsistency with which they are used, and also comparing adult and child learners. In Experiment 1 we presented adult learners with "scattered inconsistency"--the use of multiple determiners varying in frequency in the same context--and found that adults will reproduce these inconsistencies at low levels of scatter, but at very high levels of scatter will regularize the determiner system, producing the most frequent determiner form almost all the time. In Experiment 2 we showed that this is not merely the result of frequency: when determiners are used with low frequencies but in consistent contexts, adults will learn all of the determiners veridically. In Experiment 3 we compared adult and child learners, finding that children will almost always regularize inconsistent forms, whereas adult learners will only regularize the most complex inconsistencies. Taken together, these results suggest that regularization processes in natural language learning, such as those seen in the acquisition of language from non-native speakers or in the formation of young languages, may depend crucially on the nature of language learning by young children. (Contains 1 table and 13 figures.)},
	Author = {Hudson Kam, Carla L. and Newport, Elissa L.},
	Date-Added = {2013-03-10 22:53:03 +0000},
	Date-Modified = {2014-05-10 20:06:43 +0000},
	Doi = {10.1016/j.cogpsych.2009.01.001},
	Journal = {Cognitive Psychology},
	Keywords = {acquisition, artificial language, second language, variability},
	Month = {August},
	Number = {1},
	Pages = {30-66},
	Title = {Getting It Right by Getting It Wrong: When Learners Change Languages},
	Volume = {59},
	Year = {2009},
