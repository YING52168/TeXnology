@article{messenger2011,
	Abstract = {In a syntactic priming paradigm, three- and four-year-old children and adults described transitive events after hearing thematically and lexically unrelated active and short passive prime descriptions. Both groups were more likely to produce full passive descriptions (\emph{the king is being scratched by the tiger}) following short passive primes (\emph{the girls are being shocked}) than active primes (\emph{the sheep is shocking the girl}). These results suggest that by four, children have (shared) abstract syntactic representations for both short and full passives, contrary to previous proposals (e.g., Horgan, 1978).},
