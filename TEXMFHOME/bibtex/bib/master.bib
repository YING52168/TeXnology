@article{crain2009,
	Abstract = {In the 1980s, researchers in child language devised several new experimental techniques to assess children's emerging linguistic competence. Innovations in methodology were needed to bridge the apparent gap between the expectation of rapid language acquisition, based on linguistic theory, and the protracted acquisition that was being witnessed using the tools and tasks available at the time. This article discusses the use of elicited production in charting the course of development of verbal passives, a linguistic structure that was thought to be late developing (e.g., Borer & Wexler 1987). We compare the findings from an elicited production study with the findings from other tasks. We conclude that verbal passives emerge earlier in children's grammar than had been previously recognized, thereby bringing data from experimental studies of child language more in line with the expectations of linguistic theory.},
	Author = {Crain, Stephen and Thornton, Rosalind and Murasugi, Keiko},
	Date-Added = {2013-10-08 05:37:06 +0000},
